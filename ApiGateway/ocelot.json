      "DownstreamHostAndPorts": [
        {
          "Host": "notificationwebapi",
          "Port": 8080
        }
      ],
      "DownstreamPathTemplate": "/api/notification/receiver/{everything}"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8001"
  }
}